{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run CrushDB",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/CrushDB/bin/Debug/net7.0/CrushDB.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/CrushDB",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Run CrushDB.Tests.Performance",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Release",
            "program": "${workspaceFolder}/tests/CrushDB.Tests.Performance/bin/Release/net7.0/CrushDB.Tests.Performance.dll",
            "args": [
                "--filter",
                "*"
            ],
            "cwd": "${workspaceFolder}/tests/CrushDB.Tests.Performance",
            "console": "internalConsole",
            "stopAtEntry": false
        },
    ]
}